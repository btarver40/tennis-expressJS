<html>
  <head>
    <% include ./partials/head/ %>
  </head>
  <body>
    <header>
      <% include ./partials/header %>
    </header>
    <div class="container">
      <% var url="tennis" + tennis.id + "?_method=PUT" %>
      <form method="POST" action=<%= url %> 
      <input required name="title" value=<%= tennis.title %> />
      <a href="/tennis" class="btn red">Cancel</a>
      <button class="btn">Save</button>
      </form>
    </div>
  </body>
</html>